{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.3.3"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/api/404.php" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "s-maxage=1, stale-while-revalidate" }] }
  ],
  "outputDirectory": "public",
  "public": true
}
